<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="443.0" prefWidth="716.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DiagramWindow">
<!--<BorderPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DiagramWindow">-->
   <left>
      <VBox fx:id="boxForCheckCombos" prefHeight="400.0" prefWidth="279.0" styleClass="custom-dashed-border-green" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="showDiagramBtn" mnemonicParsing="false" onAction="#showDiagram" text="show Diagram" />
         </children></VBox>
   </left>
   <center>
      <StackPane prefHeight="400.0" prefWidth="505.0" styleClass="custom-dashed-border-blue" BorderPane.alignment="CENTER">
         <children>
            <Pane fx:id="paneForDiagram" prefHeight="200.0" prefWidth="200.0" />
            <TabPane fx:id="tabPane" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" visible="false" />
         </children></StackPane>
   </center>
   <top>
      <HBox alignment="CENTER" prefHeight="38.0" prefWidth="784.0" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <RadioButton mnemonicParsing="false" onAction="#inTabsClick" text="in tabs">
               <toggleGroup>
                  <ToggleGroup fx:id="tabs" />
               </toggleGroup></RadioButton>
            <RadioButton mnemonicParsing="false" onAction="#allInOneClick" selected="true" text="all in one" toggleGroup="$tabs" />
         </children>
      </HBox>
   </top>
</BorderPane>
